#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
	int a,b;
    float c,d;
    scanf("%d%d",&a,&b);
    scanf("%f%f",&c,&d);
    int result=a+b;
    int diff=a-b;
    printf("%d %d\n",result,diff);
    float res=c+d;
    float dif=c-d;
    printf("%0.1f %0.1f",res,dif);
    

    
    
    
    return 0;
}
